<sap:WorkflowElementDialog x:Class="Plugins.Shared.Library.UserControls.VerifyVariableDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Plugins.Shared.Library.UserControls"
             xmlns:sap="clr-namespace:System.Activities.Presentation;assembly=System.Activities.Presentation" 
             mc:Ignorable="d" 
             Title="工作流设计器" 
             WindowResizeMode="NoResize" 
             Height="55" Width="350">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="42" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Image Grid.Column="0" Height="32" Width="32" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="3 0 0 0" Source="pack://application:,,,/Resource/Image/Designer/VerityVariableWrong.png" />
        <StackPanel Grid.Column="1" Margin="0 5 0 0">
            <TextBlock Text="Name:" />
            <TextBlock x:Name="_description" TextWrapping="Wrap" />
        </StackPanel>
    </Grid>
</sap:WorkflowElementDialog>
