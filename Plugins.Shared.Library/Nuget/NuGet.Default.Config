<?xml version="1.0" encoding="utf-8"?>
<!--该配置文件保存默认包源，请勿手动修改-->
<configuration>
  <packageSources>
    <!-- Nuget默认包源地址-->
    <!--<add key="本地（内置包仅供测试）" value="Packages" />-->
  </packageSources>
  <config>
    <!-- 指定全局包下载路径 -->
    <add key="globalPackagesFolder" value="%LocalAppData%\UniStudio\Packages\.nuget\packages" />
  </config>
</configuration>
